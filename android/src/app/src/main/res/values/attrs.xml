<?xml version="1.0" encoding="utf-8"?>
<resources>
	<attr name="buttonType">
		<enum name="primary" value="1" />
		<enum name="primary_highlighted" value="2" />
		<enum name="primary_warning" value="3" />
		<enum name="secondary_black" value="4" />
		<enum name="secondary_white" value="5" />
	</attr>
	<declare-styleable name="Button">
		<attr name="buttonType" />
	</declare-styleable>
	<declare-styleable name="CheckBox">
		<attr name="is_dash" format="boolean" />
	</declare-styleable>
	<declare-styleable name="SwitchControl">
		<attr name="switch_enabled" format="boolean" />
		<attr name="switch_checked" format="boolean" />
		<attr name="switch_left_label" format="string" default="Off"/>
		<attr name="switch_right_label" format="string" default="On"/>
	</declare-styleable>
	<declare-styleable name="LabelEditText">
		<attr name="label_text_text" format="string" />
		<attr name="label_text_label" format="string" />
		<attr name="label_text_enabled" format="boolean" />
	</declare-styleable>
	
	<attr name="togglePosition">
		<enum name="left" value="1" />
		<enum name="middle" value="2" />
		<enum name="right" value="3" />
	</attr>
	<declare-styleable name="ToggleButton">
		<attr name="togglePosition" />
	</declare-styleable>
	
	<attr name="imageType">
		<enum name="custom" value="0" />
		<enum name="close" value="1" />
		<enum name="list" value="2" />
		<enum name="remove" value="3" />
		<enum name="levelup" value="4" />
		<enum name="sec" value="5" />
		<enum name="play" value="6" />
	</attr>
	<declare-styleable name="ImageButton">
		<attr name="imageType" />
		<attr name="enabled" format="boolean" />
	</declare-styleable>
	<declare-styleable name="Sliderbar">
		<attr name="min_value" format="integer"/>
		<attr name="max_value" format="integer"/>
		<attr name="divide_in" format="integer" />
		<attr name="current_value" format="integer" />
	</declare-styleable>
	
	    <declare-styleable name="ProgressBar">
        <attr name="android:progressDrawable" />
        <attr name="android:minWidth" />
        <attr name="android:maxWidth" />
        <attr name="android:minHeight" />
        <attr name="android:maxHeight" />
        <attr name="android:max" />
        <attr name="android:progress" />
        <attr name="android:secondaryProgress" />
    </declare-styleable>

    <declare-styleable name="SeekBar">
        <attr name="android:thumb" />
        <attr name="android:thumbOffset" />
    </declare-styleable>
    <declare-styleable name="Theme">
        <attr name="android:disabledAlpha" />
    </declare-styleable>

	<attr name="renderMode" format="enum">
		<enum name="use_background_thread" value="0"/>
		<enum name="use_main_thread" value="0"/>
	</attr>

	<attr name="domainStepMode" format="enum">
		<enum name="subdivide" value="0"/>
		<enum name="increment_by_val" value="1"/>
		<enum name="increment_by_pixels" value="2"/>
	</attr>

	<attr name="rangeStepMode" format="enum">
		<enum name="subdivide" value="0"/>
		<enum name="increment_by_val" value="1"/>
		<enum name="increment_by_pixels" value="2"/>
	</attr>

	<!-- values  must match the ordinal of its corresponding
    element in the AnchorPosition enum.  -->
	<attr name="domainLabelAnchorPosition" format="enum">
		<enum name="top_middle" value="0"/>
		<enum name="left_top" value="1"/>
		<enum name="left_middle" value="2"/>
		<enum name="left_bottom" value="3"/>
		<enum name="right_top" value="4"/>
		<enum name="right_middle" value="5"/>
		<enum name="right_bottom" value="6"/>
		<enum name="bottom_middle" value="7"/>
		<enum name="center" value="8"/>
	</attr>

	<attr name="rangeLabelAnchorPosition" format="enum">
		<enum name="top_middle" value="0"/>
		<enum name="left_top" value="1"/>
		<enum name="left_middle" value="2"/>
		<enum name="left_bottom" value="3"/>
		<enum name="right_top" value="4"/>
		<enum name="right_middle" value="5"/>
		<enum name="right_bottom" value="6"/>
		<enum name="bottom_middle" value="7"/>
		<enum name="center" value="8"/>
	</attr>

	<attr name="legendAnchorPosition" format="enum">
		<enum name="top_middle" value="0"/>
		<enum name="left_top" value="1"/>
		<enum name="left_middle" value="2"/>
		<enum name="left_bottom" value="3"/>
		<enum name="right_top" value="4"/>
		<enum name="right_middle" value="5"/>
		<enum name="right_bottom" value="6"/>
		<enum name="bottom_middle" value="7"/>
		<enum name="center" value="8"/>
	</attr>

	<attr name="domainLabelHeightSizeLayoutType" format="enum">
		<enum name="absolute" value="0"/>
		<enum name="relative" value="1"/>
		<enum name="fill" value="2"/>
	</attr>

	<attr name="domainLabelWidthSizeLayoutType" format="enum">
		<enum name="absolute" value="0"/>
		<enum name="relative" value="1"/>
		<enum name="fill" value="2"/>
	</attr>

	<attr name="rangeLabelHeightSizeLayoutType" format="enum">
		<enum name="absolute" value="0"/>
		<enum name="relative" value="1"/>
		<enum name="fill" value="2"/>
	</attr>

	<attr name="rangeLabelWidthSizeLayoutType" format="enum">
		<enum name="absolute" value="0"/>
		<enum name="relative" value="1"/>
		<enum name="fill" value="2"/>
	</attr>

	<attr name="legendHeightSizeLayoutType" format="enum">
		<enum name="absolute" value="0"/>
		<enum name="relative" value="1"/>
		<enum name="fill" value="2"/>
	</attr>

	<attr name="legendWidthSizeLayoutType" format="enum">
		<enum name="absolute" value="0"/>
		<enum name="relative" value="1"/>
		<enum name="fill" value="2"/>
	</attr>

	<attr name="legendIconHeightSizeLayoutType" format="enum">
		<enum name="absolute" value="0"/>
		<enum name="relative" value="1"/>
		<enum name="fill" value="2"/>
	</attr>

	<attr name="legendIconWidthSizeLayoutType" format="enum">
		<enum name="absolute" value="0"/>
		<enum name="relative" value="1"/>
		<enum name="fill" value="2"/>
	</attr>

	<attr name="domainLabelLayoutStyleX" format="enum">
		<enum name="absolute_from_left" value="0"/>
		<enum name="absolute_from_right" value="1"/>
		<enum name="absolute_from_center" value="2"/>
		<enum name="relative_from_left" value="3"/>
		<enum name="relative_from_right" value="4"/>
		<enum name="relative_from_center" value="5"/>
	</attr>

	<attr name="domainLabelLayoutStyleY" format="enum">
		<enum name="absolute_from_top" value="0"/>
		<enum name="absolute_from_bottom" value="1"/>
		<enum name="absolute_from_center" value="2"/>
		<enum name="relative_from_top" value="3"/>
		<enum name="relative_from_bottom" value="4"/>
		<enum name="relative_from_center" value="5"/>
	</attr>

	<attr name="rangeLabelLayoutStyleX" format="enum">
		<enum name="absolute_from_left" value="0"/>
		<enum name="absolute_from_right" value="1"/>
		<enum name="absolute_from_center" value="2"/>
		<enum name="relative_from_left" value="3"/>
		<enum name="relative_from_right" value="4"/>
		<enum name="relative_from_center" value="5"/>
	</attr>

	<attr name="rangeLabelLayoutStyleY" format="enum">
		<enum name="absolute_from_top" value="0"/>
		<enum name="absolute_from_bottom" value="1"/>
		<enum name="absolute_from_center" value="2"/>
		<enum name="relative_from_top" value="3"/>
		<enum name="relative_from_bottom" value="4"/>
		<enum name="relative_from_center" value="5"/>
	</attr>

	<attr name="legendLayoutStyleX" format="enum">
		<enum name="absolute_from_left" value="0"/>
		<enum name="absolute_from_right" value="1"/>
		<enum name="absolute_from_center" value="2"/>
		<enum name="relative_from_left" value="3"/>
		<enum name="relative_from_right" value="4"/>
		<enum name="relative_from_center" value="5"/>
	</attr>

	<attr name="legendLayoutStyleY" format="enum">
		<enum name="absolute_from_top" value="0"/>
		<enum name="absolute_from_bottom" value="1"/>
		<enum name="absolute_from_center" value="2"/>
		<enum name="relative_from_top" value="3"/>
		<enum name="relative_from_bottom" value="4"/>
		<enum name="relative_from_center" value="5"/>
	</attr>

	<!-- Plot -->
	<declare-styleable name="Plot">
		<attr name="renderMode"/>
		<attr name="label" format="string"/>
		<attr name="labelTextSize" format="dimension"/>
		<attr name="labelTextColor" format="color"/>
		<attr name="backgroundColor" format="color"/>
		<attr name="borderColor" format="color"/>
		<attr name="borderThickness" format="dimension"/>
	</declare-styleable>

	<!-- XYPlot -->
	<declare-styleable name="xy.XYPlot" parent="@style/Plot">

		<attr name="domainStepMode"/>
		<attr name="domainStep" format="dimension|float|integer"/>

		<attr name="rangeStepMode"/>
		<attr name="rangeStep" format="dimension|float|integer"/>

		<!-- domain label -->
		<attr name="domainLabel" format="string"/>
		<attr name="domainLabelTextColor" format="color"/>
		<attr name="domainLabelTextSize" format="dimension"/>
		<attr name="domainLabelHeightSizeLayoutType"/>
		<attr name="domainLabelWidthSizeLayoutType"/>
		<attr name="domainLabelHeight" format="dimension|float|integer"/>
		<attr name="domainLabelWidth" format="dimension|float|integer"/>
		<attr name="domainLabelLayoutStyleX"/>
		<attr name="domainLabelLayoutStyleY"/>
		<attr name="domainLabelPositionX" format="dimension|float|integer"/>
		<attr name="domainLabelPositionY" format="dimension|float|integer"/>
		<attr name="domainLabelAnchorPosition"/>
		<attr name="domainLabelVisible" format="boolean"/>

		<!-- range label -->
		<attr name="rangeLabel" format="string"/>
		<attr name="rangeLabelTextColor" format="color"/>
		<attr name="rangeLabelTextSize" format="dimension"/>
		<attr name="rangeLabelHeightSizeLayoutType"/>
		<attr name="rangeLabelWidthSizeLayoutType"/>
		<attr name="rangeLabelHeight" format="dimension|float|integer"/>
		<attr name="rangeLabelWidth" format="dimension|float|integer"/>
		<attr name="rangeLabelLayoutStyleX"/>
		<attr name="rangeLabelLayoutStyleY"/>
		<attr name="rangeLabelPositionX" format="dimension|float|integer"/>
		<attr name="rangeLabelPositionY" format="dimension|float|integer"/>
		<attr name="rangeLabelAnchorPosition"/>
		<attr name="rangeLabelVisible" format="boolean"/>

		<!-- graph widget margin / padding -->
		<attr name="graphMarginTop" format="dimension"/>
		<attr name="graphMarginBottom" format="dimension"/>
		<attr name="graphMarginLeft" format="dimension"/>
		<attr name="graphMarginRight" format="dimension"/>
		<attr name="graphPaddingTop" format="dimension"/>
		<attr name="graphPaddingBottom" format="dimension"/>
		<attr name="graphPaddingLeft" format="dimension"/>
		<attr name="graphPaddingRight" format="dimension"/>
		<attr name="gridMarginTop" format="dimension"/>

		<!-- grid rect margin / padding -->
		<attr name="gridMarginBottom" format="dimension"/>
		<attr name="gridMarginLeft" format="dimension"/>
		<attr name="gridMarginRight" format="dimension"/>
		<attr name="gridPaddingTop" format="dimension"/>
		<attr name="gridPaddingBottom" format="dimension"/>
		<attr name="gridPaddingLeft" format="dimension"/>
		<attr name="gridPaddingRight" format="dimension"/>

		<attr name="graphBackgroundColor" format="color"/>
		<attr name="graphDomainLineThickness" format="dimension"/>
		<attr name="graphRangeLineThickness" format="dimension"/>
		<attr name="graphDomainLineColor" format="color"/>
		<attr name="graphRangeLineColor" format="color"/>
		<attr name="rangeTickLabelTextSize" format="dimension"/>
		<attr name="domainTickLabelTextSize" format="dimension"/>
		<attr name="rangeTickLabelTextColor" format="color"/>
		<attr name="domainTickLabelTextColor" format="color"/>
		<attr name="rangeOriginTickLabelTextSize" format="dimension"/>
		<attr name="domainOriginTickLabelTextSize" format="dimension"/>
		<attr name="rangeOriginTickLabelTextColor" format="color"/>
		<attr name="domainOriginTickLabelTextColor" format="color"/>
		<attr name="gridBackgroundColor" format="color"/>

		<!-- legend widget -->
		<attr name="legendHeightSizeLayoutType"/>
		<attr name="legendWidthSizeLayoutType"/>
		<attr name="legendHeight" format="dimension|float|integer"/>
		<attr name="legendWidth" format="dimension|float|integer"/>
		<attr name="legendLayoutStyleX"/>
		<attr name="legendLayoutStyleY"/>
		<attr name="legendPositionX" format="dimension|float|integer"/>
		<attr name="legendPositionY" format="dimension|float|integer"/>
		<attr name="legendAnchorPosition"/>
		<attr name="legendTextSize" format="dimension"/>
		<attr name="legendTextColor" format="color"/>
		<attr name="legendIconHeightSizeLayoutType"/>
		<attr name="legendIconWidthSizeLayoutType"/>
		<attr name="legendIconHeight" format="dimension|float|integer"/>
		<attr name="legendIconWidth" format="dimension|float|integer"/>
		<attr name="legendVisible" format="boolean"/>
	</declare-styleable>

	<!-- TODO: PieChart -->
	<declare-styleable name="pie.PieChart" parent="@style/Plot">
		<attr name="pieBorderColor" format="color"/>
		<attr name="pieBorderThickness" format="dimension"/>
	</declare-styleable>

</resources>